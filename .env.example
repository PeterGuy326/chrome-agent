# Chrome Agent 环境配置

# 服务器配置
PORT=3000
HOST=0.0.0.0

# AI配置
AI_PROVIDER=bailian
AI_MODEL=qwen-turbo
AI_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
AI_API_KEY=your_api_key_here
AI_TEMPERATURE=0.2
AI_MAX_TOKENS=2048
AI_TOP_P=1
AI_TIMEOUT=90000
AI_INTENT_MODEL=qwen-vl-plus
AI_PLANNER_MODEL=qwen-max

# 浏览器配置
CHROME_HEADLESS=false
CHROME_TIMEOUT=30000
CHROME_STEALTH=true

# 选择器配置
SELECTOR_TIMEOUT=5000
SELECTOR_RETRIES=3

# 数据抽取配置
EXTRACTOR_TIMEOUT=10000
EXTRACTOR_RETRIES=3
EXTRACTOR_ENABLE_CACHE=true

# 存储配置
STORAGE_BASE_DIR=./data
STORAGE_ENABLE_CACHE=true
STORAGE_CACHE_SIZE=1000

# 缓存配置
CACHE_TTL=3600000
CACHE_MAX_SIZE=1000
